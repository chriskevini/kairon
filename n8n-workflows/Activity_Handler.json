    {
      "parameters": {
        "jsCode": "const categories = $input.all().map(item => item.json.name).join(', ');\nconst event = $('Execute Workflow Trigger').first().json.event;\n\nreturn [{\n  json: {\n    categories,\n    ...event\n  }\n}];"
