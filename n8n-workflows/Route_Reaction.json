    {
      "parameters": {
        "jsCode": "// Check if projection can be regenerated\nconst ctx = $('Receive Event').first().json.ctx;\nconst projection = $json;\n\n// Only daily_summary and activity_reminder can be regenerated\nconst regeneratableTypes = ['daily_summary', 'activity_reminder'];\n\nif (!projection || !projection.projection_id) {\n  return {\n    ctx,\n    can_regenerate: false,\n    error_message: 'No projection found for this message'\n  };\n}\n\nif (!regeneratableTypes.includes(projection.projection_type)) {\n  return {\n    ctx,\n    can_regenerate: false,\n    error_message: `Cannot regenerate ${projection.projection_type} projections`\n  };\n}\n\nreturn {\n  ctx,\n  can_regenerate: true,\n  projection: projection,\n  regenerate_type: projection.projection_type === 'daily_summary' ? 'summary' : 'reminder'\n};"
      },
