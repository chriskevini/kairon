      "parameters": {
        "operation": "executeQuery",
        "query": "-- Find and void projections for this message\n-- The reaction's message_id is the target message that created the projection\n-- Message events store discord_message_id in payload\nUPDATE projections p\nSET \n  status = 'voided',\n  voided_at = NOW(),\n  voided_reason = 'user_rejected',\n  voided_by_event_id = $1::uuid\nFROM events e\nWHERE p.event_id = e.id\n  AND e.payload->>'discord_message_id' = $2\n  AND p.status IN ('pending', 'auto_confirmed', 'confirmed')\nRETURNING p.id, p.projection_type, p.data->>'description' as description, p.data->>'text' as text;",
        "options": {
          "queryReplacement": "={{ $json.ctx.event.event_id }},={{ $json.ctx.event.message_id }}"
        }
      },
