      "parameters": {
        "operation": "executeQuery",
        "query": "-- Find projection and its type by the Discord message that created it\n-- The reaction's message_id is the bot's reply message\n-- We stored discord_message_id in projection.data\nSELECT \n  p.id as projection_id,\n  p.projection_type,\n  p.trace_id,\n  p.event_id,\n  p.data\nFROM projections p\nWHERE p.data->>'discord_message_id' = $1\n  AND p.status IN ('auto_confirmed', 'confirmed')\nLIMIT 1;",
        "options": {
          "queryReplacement": "={{ $json.ctx.event.message_id }}"
        }
